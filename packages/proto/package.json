{
  "name": "@valorem-labs-inc/proto",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm codegen && pnpm tsup",
    "clean": "rm -rf ./dist && rm -rf ./src/generated",
    "codegen": "npx buf generate",
    "docs": "typedoc --out docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "git submodule update --init --recursive",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@bufbuild/buf": "^1.20.0-1",
    "@bufbuild/protobuf": "^1.2.1"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.2.1",
    "@connectrpc/protoc-gen-connect-es": "^1.1.2",
    "@types/node": "^20.5.2",
    "dedent": "^1.5.1",
    "eslint-config-custom": "workspace:*",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
